public function <%- name %> (<%- input %>)
<%if (tags != '') {%> <%- tags %> <% } %> <%if (output != '') {%> returns (<%- output %>) <% } %>
{
    require(state == States.<%- src %>);
  <%if (guards != '') {%> //Guards
    require(<%- guards %>); <% } %>  <%if (statements != '') {%>
    //Actions
    <%- statements %> <% } %>  <%if (src != dst) {%>
    //State change
    state = States.<%- dst %>; <% } %>
}
